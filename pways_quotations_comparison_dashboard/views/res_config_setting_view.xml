<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form_purchase_set_cancel" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.cancel</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="purchase.res_config_settings_view_form_purchase"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='auto_receipt_reminder']" position="after">
                <div class="col-12 col-lg-6 o_setting_box">
                    <div class="o_setting_left_pane">
                        <field name="is_remaing_cancel"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="is_remaing_cancel"/>
                        <div class="text-muted">
                            Cancel Non Selected RFQ
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
        </record>
    </data>
</odoo>
